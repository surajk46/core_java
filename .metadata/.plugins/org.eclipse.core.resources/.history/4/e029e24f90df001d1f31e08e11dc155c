package p3_account;

public abstract class Account {
	private int accno;
	private String name;
	private double balance;
	
	public double getBalance()
	{
		return balance;
	}
	public Account() {
		super();
	}


	public Account(int accno, String name, double balance) {
		super();
		this.accno = accno;
		this.name = name;
		this.balance = balance;
	}

	public abstract void withdraw(double amt) ;
	
}
