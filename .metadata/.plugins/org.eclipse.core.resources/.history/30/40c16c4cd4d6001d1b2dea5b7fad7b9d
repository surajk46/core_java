package p4_practice;

public class Date {
	private int dd;
	private int mm;
	private int yy;
	
	public Date()
	{
		dd=0;
		mm=0;
		yy=0;
	}
	public Date(int dd,int mm, int yy)
	{
		this.dd=dd;
		this.mm=mm;
		this.yy=yy;
	}
	
	public   String toString() 
	{
		return "\nDD = "+dd+"\nmm = "+mm+"\nyy = "+yy+ "\n"
	}
	
}
