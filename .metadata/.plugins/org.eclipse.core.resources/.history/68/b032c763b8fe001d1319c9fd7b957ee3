package countCharacter;

import java.util.HashMap;
import java.util.Map;

public class Demo {
	public static void main(String[] args) {
		String a="coccacola";
		char ch;
		Map<Character, Integer> m=new HashMap<>();
		for(int i=0;i<a.length();i++)
		{
			ch=a.charAt(i);
			for(int j=i;j<a.length();j++)
			{
				if(ch==a.charAt(j));
			}
		}
	}
}
